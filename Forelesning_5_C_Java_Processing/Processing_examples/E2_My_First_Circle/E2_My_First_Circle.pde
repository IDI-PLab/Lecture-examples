// My First Circle
//

void setup() {                // Called once when the program is started.
  size(200, 200);             // Canvas size is 200 x 200 pixels.
  background(128);            // Background color is grey (128).
}

void draw() {                  // Called for every frame
  stroke(0);                  // Stroke color is black (0)
  fill(255);                  // Fill color is white (255)
  ellipse(100,100,50,50);   // Draw a circle center (100,100), size (50x50)
}


